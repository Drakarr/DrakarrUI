{
  "name": "drakarrui",
  "author": "Drakarr",
  "description": "Vue component library based on DaisyUI and TailwindCSS",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/drakarrui.umd.js",
  "module": "./dist/drakarrui.es.js",
  "exports": {
    ".": {
      "import": "./dist/drakarrui.es.js",
      "require": "./dist/drakarrui.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preserve": "vite build",
    "serve": "vite preview --port 3000",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "daisyui": "^2.20.0",
    "drakarrui": "file:drakarrui-0.0.1.tgz",
    "tailwindcss": "^3.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "sass": "^1.54.0",
    "vite": "^3.0.3",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  }
}